if test -n "$1"; then
  prefix="$1"
fi
test -n "$prefix" || prefix=/usr

bindir="$prefix/bin"
mkdir -p "$bindir" || exit 1

for name in \
  drop-long-lines \
  files-filter \
  replace-string \
  rate \
  lines-filter \
  splice \
  is-empty-directory \
  unique-name \
  line-length \
  stemname \
  rename-lowercase \
  group
do
  cp "$PWD/exe/compiled/$name" "$bindir/$name" || exit 1
done
